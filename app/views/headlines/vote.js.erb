$(".headline-row-<%= @headline.id %>").replaceWith("<%=j render partial: 'headlines/headline', locals: {headline: @headline, vote_statuses: VoteStatusCollection.new(current_user, [@headline])} %>");
$("#show-vote-count").html("<%= pluralize @headline.vote_count, "vote" %>");
$("#show-vote-box").html("<%=j render partial: "headlines/headline_votebox", locals: {headline: @headline, vote_statuses: VoteStatusCollection.new(current_user, [@headline])} %>");

